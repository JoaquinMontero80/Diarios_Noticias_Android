<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ImageView
        android:id="@+id/imageExpansion"
        android:layout_width="298dp"
        android:layout_height="162dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/imageCincodias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.67"
        app:srcCompat="@drawable/portada_la_expansion" />

    <ImageView
        android:id="@+id/imageCincodias"
        android:layout_width="235dp"
        android:layout_height="143dp"
        android:layout_marginBottom="24dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/imagePortafolio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.431"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/portada_cinco_dias" />

    <ImageView
        android:id="@+id/imagePortafolio"
        android:layout_width="375dp"
        android:layout_height="123dp"
        android:layout_marginBottom="28dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/imageElConfidencial"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/portafolio" />

    <ImageView
        android:id="@+id/imageElConfidencial"
        android:layout_width="371dp"
        android:layout_height="131dp"
        android:layout_marginBottom="24dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/portada_el_confidencial" />

</androidx.constraintlayout.widget.ConstraintLayout>